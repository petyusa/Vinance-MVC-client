@using Vinance.Contracts.Enumerations
@using Vinance.Web.Components.Account
@using Vinance.Web.Components.Category
<div class="row">
    <div class="col-lg-6">
        <div class="row">
            <div class="col-12">
                @await Component.InvokeAsync(typeof(GetAllAccount), new { editable = false })
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                @await Component.InvokeAsync(typeof(CategoryChart))
            </div>
        </div>
    </div>
    <div class="col-lg-6">
        @await Component.InvokeAsync(typeof(GetAllCategory), new { editable = false, type = CategoryType.Expense })
    </div>
</div>
