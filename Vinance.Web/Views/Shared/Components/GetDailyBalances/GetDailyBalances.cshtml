@using Newtonsoft.Json
@model Dictionary<DateTime, int>

<canvas id="daily-balance-chart"></canvas>
<script>
    var labels = @Html.Raw(JsonConvert.SerializeObject(Model.Select(x => x.Key)));
    var chartData = @Html.Raw(JsonConvert.SerializeObject(Model.Select(x => x.Value)));

    vinance.createDailyBalanceChart("daily-balance-chart", labels, chartData);
</script>
